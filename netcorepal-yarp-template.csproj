<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
	  <PackageType>Template</PackageType>
	  <PackageId>NetCorePal.YARP.Template</PackageId>
	  <Title>NetCorePal YARP Ingress Controller Templates</Title>
	  <Authors>NetCorePal</Authors>
	  <Description>NetCorePal YARP Ingress Controller Templates</Description>
	  <PackageTags>dotnet-new;templates</PackageTags>
	  <TargetFrameworks>net6.0;net7.0;net8.0</TargetFrameworks>
	  <IncludeContentInPack>true</IncludeContentInPack>
	  <IncludeBuildOutput>false</IncludeBuildOutput>
	  <ContentTargetFolders>content</ContentTargetFolders>
	  <NoDefaultExcludes>true</NoDefaultExcludes>
	  <VersionPrefix>1.1.0</VersionPrefix>
	</PropertyGroup>
  
	<ItemGroup>
	  <Content Include="template\**\*" Exclude="template\**\bin\**;template\**\obj\**;template\.vs\**" />
	  <Compile Remove="**\*" />
	</ItemGroup>
  
  </Project>